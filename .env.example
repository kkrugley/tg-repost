# Telegram Bot API (бот администратор в обоих каналах)
TELEGRAM_BOT_TOKEN=your_bot_token
TARGET_CHANNEL_ID=-1001234567890

# Канал-источник
SOURCE_CHANNEL=pulkrug

# Диапазон дат
START_DATE=2022-10-30
END_DATE=2024-10-24
TIMEZONE=UTC

# Supabase (используйте Transaction pooler, IPv4)
# Project Overview -> Connect -> Connection string -> URI -> Method: Transaction pooler
# Пример: postgresql://postgres.<project-id>:<password>@aws-0-<region>.pooler.supabase.com:6543/postgres?sslmode=require
DATABASE_URL=postgresql://postgres:password@aws-0-xx.pooler.supabase.com:6543/postgres?sslmode=require
DATABASE_SSL=true
# Опционально: таймауты и кэш
# DATABASE_CONNECT_TIMEOUT=10.0
# DATABASE_COMMAND_TIMEOUT=60.0
# DATABASE_DISABLE_STATEMENT_CACHE=true
# MAX_RETRIES=1
# RETRY_DELAY_SECONDS=5
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your_anon_or_service_key

# Веб-сервер
PORT=10000
LOG_LEVEL=INFO
